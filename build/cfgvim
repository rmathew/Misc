#!/bin/bash

# Configure a no-nonsense, text-oriented build of Vim.
# (Edit "src/feature.h" as needed.)
./configure --prefix=/usr --with-compiledby="Ranjit Mathew" --disable-gui \
    --disable-gpm --disable-xsmp --disable-selinux --disable-nls --disable-xim \
    --without-x --without-gnome --with-features=big
